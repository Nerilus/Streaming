er/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/context.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/GlobalStyle.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Login.js",["152"],"import React, { useState, useContext } from 'react';\nimport { useNavigate } from 'react-router-dom';\nimport API from '../API';\n// Components\nimport Button from './Button';\n// Styles\nimport { Wrapper } from './Login.styles';\n// Context\nimport { Context } from '../context';\n\nconst Login = () => {\n  const [username, setUsername] = useState('');\n  const [password, setPassword] = useState('');\n  const [error, setError] = useState(false);\n\n  const [_user, setUser] = useContext(Context);\n  const navigate = useNavigate();\n\n  const handleSubmit = async () => {\n    setError(false);\n    try {\n      const requestToken = await API.getRequestToken();\n      const sessionId = await API.authenticate(\n        requestToken,\n        username,\n        password\n      );\n      console.log(sessionId);\n      setUser({ sessionId: sessionId.session_id, username });\n\n      navigate('/');\n    } catch (error) {\n      setError(true);\n    }\n  };\n\n  const handleInput = e => {\n    const name = e.currentTarget.name;\n    const value = e.currentTarget.value;\n\n    if (name === 'username') setUsername(value);\n    if (name === 'password') setPassword(value);\n  };\n\n  return (\n    <Wrapper>\n      {error && <div className='error'>There was an error!</div>}\n      <label>Username:</label>\n      <input\n        type='text'\n        value={username}\n        name='username'\n        onChange={handleInput}\n      />\n      <input\n        type='password'\n        value={password}\n        name='password'\n        onChange={handleInput}\n      />\n      <Button text='Login' callback={handleSubmit} />\n    </Wrapper>\n  );\n};\n\nexport default Login;\n","/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Home.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/NotFound.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Movie.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Header/index.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/API.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Login.styles.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/config.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/hooks/useMovieFetch.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/hooks/useHomeFetch.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Button/index.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Grid/index.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Header/Header.styles.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/HeroImage/index.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/Spinner/index.js",[],"/Users/user/REACT/react-rmdb-v3-starter-files-master/4. Finished App/react-rmdb-finished-rating/src/components/SearchBar/index.js",[],"/Users/user/REACT/react-rmdb-v3-s